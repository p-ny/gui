splice(key, 1)\r\n      }\r\n    },\r\n    setData(state, { ItemCostByName, Money }){\r\n      state.money = Money;\r\n      \r\n      state.items.forEach(item => {\r\n        const cost = ItemCostByName[item.unicName];\r\n        item.cost = cost;\r\n      });\r\n    }\r\n  },\r\n  actions: {\r\n  },\r\n  modules: {\r\n  }\r\n}"]}]}