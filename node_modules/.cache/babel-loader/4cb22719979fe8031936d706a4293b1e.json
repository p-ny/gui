,GAAA,KAAA,KAAA,GAAA,GAAA,CAAA,CArBA,CAsBA;AACA,OAvBA,MAuBA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,sBAAA;AACA;AACA;AA7BA,GAhBA;AA+CA,EAAA,OA/CA,qBA+CA;AACA,SAAA,KAAA,GAAA,CAAA,KAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,GAAA,EAAA,IAAA,IAAA;AACA,SAAA,QAAA,GAAA,WAAA,CAAA,KAAA,IAAA,EAAA,IAAA,CAAA;AACA,GAlDA;AAmDA,EAAA,aAnDA,2BAmDA;AACA,IAAA,aAAA,CAAA,KAAA,QAAA,CAAA;AACA;AArDA,CAAA","sourcesContent":["<template>\r\n    <div class=\"artimer\">\r\n        <img src=\"/img/hud/artimer.svg\" alt=\"artimer\" class=\"artimer-img\">\r\n        <div class=\"artimer-tittle\">{{loc('arrets:timer:tit')}}</div>\r\n        <div class=\"artimer-timer\">{{minutes}}:{{seconds}}</div>\r\n        <div class=\"artimer-progress\">\r\n            <div class=\"artimer-progress-thumb\" :style=\"{'width': `${progress}%`}\"></div>\r\n        </div>\r\n        <img src=\"/img/hud/arwire.png\" alt=\"artimer\" class=\"artimer-wire\">\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nexport default {\r\n    props:{\r\n        timer: Object,\r\n    },\r\n    data() {\r\n        return {            \r\n            interval: 0,\r\n            minutes: '00',\r\n            seconds: '00',\r\n            total: 100,\r\n            progress: 100,\r\n        }\r\n    },\r\n    computed: {\r\n        ...mapGetters('localization', ['loc'])\r\n    },\r\n    methods: {\r\n        tick(){\r\n            const interval = (this.timer.expiried - Date.now()) / 1000;\r\n            if(interval > 0){\r\n                let min = `${Math.floor(interval / 60)}`\r\n                if(min == '0'){\r\n                    min = '00';\r\n                }\r\n                else{\r\n                    while (min.length < 2) {\r\n                        min = \"0\" + min;\r\n                    }\r\n                }\r\n                let seconds = `${Math.floor(interval % 60)}`\r\n                if(seconds == '0'){\r\n                    seconds = '00';\r\n                }\r\n                else{\r\n                    while (seconds.length < 2) {\r\n                        seconds = \"0\" + seconds;\r\n                    }\r\n                }\r\n                this.minutes = min;\r\n                this.seconds = seconds;\r\n                this.progress = Math.floor( interval / this.total * 100);\r\n                //window.console.log(this.progress)\r\n            }else{\r\n                this.$store.commit('hud/resetArrestTimer')\r\n            }            \r\n        }\r\n    },\r\n    mounted() {\r\n        this.total = (this.timer.expiried - Date.now()) / 1000;\r\n        this.interval = setInterval(this.tick, 1000)\r\n    },\r\n    beforeDestroy(){\r\n        clearInterval(this.interval)\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    .artimer{\r\n        position: fixed;\r\n        text-transform: uppercase;\r\n        top: 0;\r\n        left: 50%;\r\n        color: #fff;\r\n        &-img{\r\n            width: 8rem;\r\n            height: 8rem;\r\n            position: absolute;\r\n        }\r\n        &-tittle{\r\n            margin-top: 1.8rem;\r\n            width: 8rem;\r\n            text-align: center;\r\n            font-size: 1.2rem;\r\n        }\r\n        &-timer{\r\n            width: 8rem;\r\n            text-align: center;\r\n            font-size: 3rem;\r\n            line-height: 2.6rem;\r\n            font-weight: 300;\r\n        }\r\n        &-progress{\r\n            width: 13rem;\r\n            margin-left: -2.5rem;\r\n            margin-top: .5rem;\r\n            height: .2rem;\r\n            background-color:rgba(#fff, .2);\r\n            position: relative;\r\n            border-radius: .2rem;\r\n            overflow: hidden;\r\n            &-thumb{\r\n                background-color:#fff;\r\n                border-radius: .2rem;\r\n                height: 100%;\r\n                transition: all 1s linear;\r\n            }\r\n        }\r\n        &-wire{\r\n            position: fixed;\r\n            bottom: 0;\r\n            right: 0;\r\n        }\r\n    }\r\n</style>"],"sourceRoot":"src/views/Hud"}]}