AA,GAAA,CAAA,QAAA,CAAA,CAAA,EAAA,CAAA,EAAA,KAAA,EAAA,MAAA;AACA,KAnBA;AAqBA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,aAAA,GAAA;AACA,QAAA,GAAA,EAAA,CAAA,IAAA,KAAA,GAAA,CAAA,CAAA,GAAA,GAAA,IAAA,KAAA,MAAA,GAAA,CAAA,GAAA;AADA,OAAA;AAGA,KAzBA;AA2BA,IAAA,SA3BA,qBA2BA,CA3BA,EA2BA;AAAA;;AAAA,kCACA,KAAA,GAAA,CAAA,qBAAA,EADA;AAAA,UACA,MADA,yBACA,GADA;;AAEA,UAAA,GAAA,GAAA,CAAA,CAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CAAA;;AAEA,UAAA,SAAA,GAAA,SAAA,SAAA,CAAA,CAAA,EAAA;AACA,YAAA,CAAA,GAAA,CAAA,CAAA,OAAA,GAAA,MAAA;;AAEA,YAAA,CAAA,GAAA,CAAA,EAAA;AACA,UAAA,CAAA,GAAA,CAAA;AACA;;AACA,YAAA,CAAA,GAAA,KAAA,CAAA,MAAA,EAAA;AACA,UAAA,CAAA,GAAA,KAAA,CAAA,MAAA;AACA;;AAEA,QAAA,KAAA,CAAA,aAAA,GAAA;AACA,UAAA,GAAA,EAAA,CAAA,GAAA,CAAA,GAAA;AADA,SAAA;AAGA,YAAA,OAAA,GAAA,GAAA,CAAA,YAAA,CAAA,CAAA,EAAA,IAAA,CAAA,GAAA,CAAA,CAAA,EAAA,KAAA,CAAA,MAAA,GAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,CAAA;;AAbA,2CAcA,OAAA,CAAA,IAdA;AAAA,YAcA,CAdA;AAAA,YAcA,CAdA;AAAA,YAcA,CAdA;;AAeA,QAAA,KAAA,CAAA,KAAA,CAAA,WAAA,EAAA;AAAA,UAAA,CAAA,EAAA,CAAA;AAAA,UAAA,CAAA,EAAA,CAAA;AAAA,UAAA,CAAA,EAAA;AAAA,SAAA;AACA,OAhBA;;AAkBA,MAAA,SAAA,CAAA,CAAA,CAAA;;AAEA,UAAA,OAAA,GAAA,SAAA,OAAA,GAAA;AACA,QAAA,QAAA,CAAA,mBAAA,CAAA,WAAA,EAAA,SAAA;AACA,QAAA,QAAA,CAAA,mBAAA,CAAA,SAAA,EAAA,OAAA;AACA,OAHA;;AAKA,MAAA,QAAA,CAAA,gBAAA,CAAA,WAAA,EAAA,SAAA;AACA,MAAA,QAAA,CAAA,gBAAA,CAAA,SAAA,EAAA,OAAA;AACA;AA1DA,GArBA;AAkFA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,WAAA;AACA,SAAA,WAAA;AACA;AArFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"hue\" @mousedown.prevent.stop=\"selectHue\">\r\n    <canvas ref=\"canvasHue\" />\r\n    <div class=\"slide\" :style=\"slideHueStyle\" ></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    hsv: {\r\n      type: Object,\r\n      default: null\r\n    },\r\n    width: {\r\n      type: Number,\r\n      default: 15\r\n    },\r\n    height: {\r\n      type: Number,\r\n    }\r\n  },\r\n\r\n  data: function() {\r\n    return {\r\n      slideHueStyle: {}\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    renderColor: function() {\r\n      const canvas = this.$refs.canvasHue\r\n      const width = this.width\r\n      const height = this.height\r\n      const ctx = canvas.getContext('2d')\r\n      canvas.width = width\r\n      canvas.height = height\r\n\r\n      const gradient = ctx.createLinearGradient(0, 0, 0, height)\r\n      gradient.addColorStop(0, '#FF0000')\r\n      gradient.addColorStop(0.17 * 1, '#FF00FF')\r\n      gradient.addColorStop(0.17 * 2, '#0000FF')\r\n      gradient.addColorStop(0.17 * 3, '#00FFFF')\r\n      gradient.addColorStop(0.17 * 4, '#00FF00')\r\n      gradient.addColorStop(0.17 * 5, '#FFFF00')\r\n      gradient.addColorStop(1, '#FF0000')\r\n      ctx.fillStyle = gradient\r\n      ctx.fillRect(0, 0, width, height)\r\n    },\r\n\r\n    renderSlide: function() {\r\n      this.slideHueStyle = {\r\n        top: (1 - this.hsv.h / 360) * this.height - 2 + 'px'\r\n      }\r\n    },\r\n\r\n    selectHue(e) {\r\n      const { top: hueTop } = this.$el.getBoundingClientRect()\r\n      const ctx = e.target.getContext('2d')\r\n\r\n      const mousemove = e => {\r\n        let y = e.clientY - hueTop\r\n\r\n        if (y < 0) {\r\n          y = 0\r\n        }\r\n        if (y > this.height) {\r\n          y = this.height\r\n        }\r\n\r\n        this.slideHueStyle = {\r\n          top: y - 2 + 'px'\r\n        }\r\n        const imgData = ctx.getImageData(0, Math.min(y, this.height - 1), 1, 1)\r\n        const [r, g, b] = imgData.data\r\n        this.$emit('selectHue', { r, g, b })\r\n      }\r\n\r\n      mousemove(e)\r\n\r\n      const mouseup = () => {\r\n        document.removeEventListener('mousemove', mousemove)\r\n        document.removeEventListener('mouseup', mouseup)\r\n      }\r\n\r\n      document.addEventListener('mousemove', mousemove)\r\n      document.addEventListener('mouseup', mouseup)\r\n    }\r\n  },\r\n\r\n  mounted: function() {\r\n    this.renderColor()\r\n    this.renderSlide()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.hue {\r\n  position: relative;\r\n  &::after {\r\n    position: absolute;\r\n    content: \"\";\r\n    width: calc(100% - 0.4rem);\r\n    height: calc(100% - 0.4rem);\r\n    border: 0.2rem solid rgba(255, 255, 255, 0.35);\r\n    top: 0;\r\n    left: 0;\r\n    border-radius: 1.1rem;\r\n    pointer-events: none;\r\n  }\r\n  canvas {\r\n    vertical-align: top;\r\n    border-radius: 1.1rem;\r\n  }\r\n  .slide {\r\n    position: absolute;\r\n    left: 50%;\r\n    top: 5rem;\r\n    width: 1.1rem;\r\n    transform: translate(-50%, 0);\r\n    border-radius: 1.1rem;\r\n    height: .2rem;\r\n    background: #fff;\r\n    box-shadow: 0 0 1px 0 rgba(0, 0, 0, 0.3);\r\n    pointer-events: none;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/CarTunningMenu/components/ColorPicker"}]}