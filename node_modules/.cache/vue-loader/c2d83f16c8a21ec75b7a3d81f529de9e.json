his.lastClick = Date.now();\r\n      switch (buttonName) {\r\n        case 'bibi':\r\n          window.mp.triggerServer('vehicle::key::changeSignal', this.remoteController.vehicleId)\r\n          break;\r\n        case 'gps':\r\n          window.mp.triggerServer('vehicle::key::enableGPS', this.remoteController.vehicleId)\r\n          break;\r\n        case 'evacuation':\r\n          window.mp.triggerServer('vehicle::key::evacCar', this.remoteController.vehicleId)\r\n          break;\r\n        case 'engine':\r\n          window.mp.triggerServer('vehicle::key::changeEngine', this.remoteController.vehicleId)\r\n          break;\r\n        case 'signalization':\r\n          window.mp.triggerServer('vehicle::key::changeSignaling', this.remoteController.vehicleId)\r\n          break;\r\n        case 'centralLock':\r\n          window.mp.triggerServer('vehicle::key::lockCar', this.remoteController.vehicleId)\r\n          break;\r\n      }\r\n    },\r\n    doorStateChange: function(doorIndex) {\r\n      if (this.lastClick + 1000 > Date.now())\r\n        return;\r\n      this.lastClick = Date.now();\r\n      window.mp.triggerServer('vehicle::key::changeDoorState', this.remoteController.vehicleId, doorIndex)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.remote-controller{\r\n  width: 10.3rem;\r\n  height: 20.75rem;\r\n  position: absolute;\r\n  right: 25rem;\r\n  bottom: 2.5rem;\r\n  padding: .5rem .5rem 0 .5rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: flex-start;\r\n  &::before{\r\n    content: '';\r\n    width: 100%;\r\n    height: 100%;\r\n    position: absolute;\r\n    top: 0;\r\n    background-image: url('/img/remoteController/bg.png');\r\n    background-size: contain;\r\n    background-repeat: no-repeat;\r\n    background-position: center;\r\n    z-index: 1;\r\n  }\r\n  svg{\r\n    width: 100%;\r\n    height: 15.35rem;\r\n    z-index: 1;\r\n  }\r\n  .icon{\r\n    pointer-events: none;\r\n    transition: .2s;\r\n    &.active{\r\n      path{\r\n        fill-opacity: 1;\r\n      }\r\n    }\r\n  }\r\n  &__button{\r\n    z-index: 1;\r\n    transition: .2s;\r\n    &:hover{\r\n      fill: rgba(255, 255, 255, 0.5);\r\n      & + .icon{\r\n        path{\r\n          fill-opacity: 1;\r\n        }\r\n      }\r\n    }\r\n    &.main-button{\r\n      &:hover{\r\n        fill: rgba(255, 255, 255, 0.05);\r\n      }\r\n    }\r\n  }\r\n  &__key-blade{\r\n    width: 5rem;\r\n    height: 10rem;\r\n    background-size: contain;\r\n    background-position: center bottom;\r\n    background-repeat: no-repeat;\r\n    background-image: url('/img/remoteController/key-blade.png');\r\n    position: absolute;\r\n    top: .5rem;\r\n    transform: translateY(0%);\r\n    z-index: 0;\r\n    transition: .5s;\r\n    &.uncovered{\r\n      transform: translateY(-100%);\r\n      transition: .1s;\r\n    }\r\n  }\r\n  &__key-btn{\r\n    width: 1rem;\r\n    height: 1rem;\r\n    border-radius: 50%;\r\n    background-size: contain;\r\n    background-position: center;\r\n    background-repeat: no-repeat;\r\n    background-image: url('/img/remoteController/btn-key.png');\r\n    position: absolute;\r\n    right: 2.5rem;\r\n    bottom: 1.5rem;\r\n    z-index: 3;\r\n  }\r\n}\r\n\r\n.controller-enter-active, .controller-leave-active {\r\n  transition: .5s;\r\n}\r\n.controller-enter, .controller-leave-to {\r\n  opacity: 0;\r\n  transform: translateY( 120%);\r\n}\r\n</style>\r\n"]}]}