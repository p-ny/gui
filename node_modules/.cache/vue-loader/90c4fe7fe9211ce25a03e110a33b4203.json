n        height: 100%;\r\n        padding-top: 4rem;\r\n        &-container{  \r\n            box-sizing: border-box;          \r\n            width: 75rem;\r\n            height:40rem;\r\n            margin: auto;\r\n            position: relative;\r\n        }\r\n        &-sub{\r\n            position: absolute;\r\n            top: 2rem;\r\n            left: 2rem;\r\n            font-size: 1rem;\r\n        }\r\n        &-type1{            \r\n            position: absolute;\r\n            top: 0;\r\n            left: 0;\r\n            &_img{\r\n                width: 22rem;\r\n                height: 29rem;\r\n            }\r\n            &_title{\r\n                position: absolute;\r\n                top: 3rem;\r\n                left: 2rem;\r\n                font-size: 3rem;\r\n            }\r\n            &:nth-child(2){\r\n                left: 23rem;\r\n            }\r\n        }\r\n        &-type2{            \r\n            position: absolute;\r\n            &_img{\r\n                width: 14rem;\r\n                height: 14rem;\r\n            }          \r\n            &_title{\r\n                position: absolute;\r\n                top: 1rem;\r\n                width: 100%;\r\n                text-align: center;\r\n                font-size: 2rem;\r\n            }\r\n            &:nth-child(3){\r\n                top: 0;\r\n                left: 46rem;\r\n            }\r\n            &:nth-child(4){\r\n                top: 0;\r\n                left: 61rem;\r\n            }\r\n            &:nth-child(5){\r\n                top: 15rem;\r\n                left: 46rem;\r\n            }\r\n            &:nth-child(6){\r\n                top: 15rem;\r\n                left: 61rem;\r\n            }\r\n        }\r\n        \r\n        &-type3{            \r\n            position: absolute;\r\n            top: 30rem;\r\n            left: 0;\r\n            &_img{\r\n                width: 45rem;\r\n                height: 9rem;\r\n            }          \r\n            &_title{\r\n                position: absolute;\r\n                top: 3rem;\r\n                left: 2rem;\r\n                font-size: 2rem;\r\n            }\r\n        }\r\n        &-type4{            \r\n            position: absolute;\r\n            top: 30rem;\r\n            left: 46rem;\r\n            &_img{\r\n                width: 29rem;\r\n                height: 9rem;\r\n            }\r\n        }\r\n        &-hover{\r\n            &:hover{\r\n                transform: scale(1.02);\r\n                img{\r\n                    box-shadow: 0 0 .5rem rgba(#D51A1B, .5);\r\n                }\r\n            }\r\n        }\r\n    }\r\n.commingsoon{\r\n    width: 100%;\r\n    height: 100%;\r\n    background: url(/img/common/commingsoon.svg) rgba(#000, .9) no-repeat center;\r\n    background-size: contain;\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    border: .1rem solid rgba(#fff, .2);\r\n}\r\n</style>"]}]}